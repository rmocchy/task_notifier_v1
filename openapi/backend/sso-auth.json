{
  "openapi": "3.1.0",
  "info": {
    "title": "sso-auth",
    "version": "1.0.0",
    "description": "sso-auth API Documentation"
  },
  "components": {
    "schemas": {},
    "parameters": {}
  },
  "paths": {
    "/auth/google/url": {
      "get": {
        "operationId": "googleAuthUri",
        "responses": {
          "200": {
            "description": "認証URLを取得",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "url": {
                      "type": "string",
                      "format": "uri"
                    }
                  },
                  "required": [
                    "url"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/auth/google/token": {
      "post": {
        "operationId": "tokenExchange",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "code": {
                    "type": "string",
                    "minLength": 1
                  }
                },
                "required": [
                  "code"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "認証コードからトークンを取得",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "access_token": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "access_token"
                  ]
                }
              }
            }
          }
        }
      }
    }
  },
  "webhooks": {}
}